                                           <div class="title" >
											 <div>
											 	<a th:href="'heuer/veranstaltungen/'+${veranstaltung.id}" th:text="${veranstaltung.title}"></a>
											 </div>
                                                     <hr>
											<div class="subtitle"> <a th:href="'heuer/veranstaltungen/'+${veranstaltung.id}" th:text="${veranstaltung.subtitle}"></a></div>
